    "@material-ui/pickers": "^3.2.10",
    "ajv": "^6.12.5",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "immer": "^5.3.6",
    "moment": "^2.24.0",
    "normalizr": "^3.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^6.8.4",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.1",
    "typeface-roboto": "^0.0.75"

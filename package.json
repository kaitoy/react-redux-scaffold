{
  "name": "react-redux-scaffold",
  "version": "2.0.0",
  "description": "React Redux Scaffold",
  "main": "src/index.tsx",
  "scripts": {
    "format": "prettier --write **/*.js **/*.tsx **/*.ts **/*.css **/*.json",
    "lint": "run-p lint:*",
    "lint:js": "eslint **/*.js **/*.tsx **/*.ts",
    "lint:css": "stylelint ./src/**/*.js ./src/**/*.tsx ./src/**/*.ts",
    "fl": "run-s format lint",
    "mock:gendb": "node genDb.js > db.json",
    "mock:server": "json-server db.json --routes jsonServerRoutes.json --port 8080",
    "mock": "run-s mock:gendb mock:server",
    "build": "webpack --config webpack.prod.js",
    "devserver": "webpack-dev-server --hot --config webpack.dev.js",
    "start": "run-p mock devserver",
    "test": "jest",
    "cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaitoy/react-redux-scaffold.git"
  },
  "author": "kaitoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaitoy/react-redux-scaffold/issues"
  },
  "homepage": "https://github.com/kaitoy/react-redux-scaffold",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
